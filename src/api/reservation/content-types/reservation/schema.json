{
  "kind": "collectionType",
  "collectionName": "reservations",
  "info": {
    "singularName": "reservation",
    "pluralName": "reservations",
    "displayName": "Reservation"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "pickup_date": {
      "type": "date",
      "required": true,
      "default": "2025-06-17"
    },
    "dropoff_date": {
      "type": "date",
      "required": true,
      "default": "2025-06-19"
    },
    "total_price": {
      "type": "decimal"
    },
    "status_r": {
      "type": "enumeration",
      "enum": [
        "pending",
        "confirmed",
        "cancelled",
        "finished"
      ]
    },
    "slug": {
      "type": "uid"
    },
    "pickup_time": {
      "type": "time",
      "required": false,
      "default": "10:00:00.000"
    },
    "dropoff_time": {
      "type": "time",
      "required": false,
      "default": "10:00:00.000"
    },
    "location": {
      "type": "string",
      "required": true
    },
    "car": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::car.car"
    },
    "customer": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::customer.customer"
    },
    "type": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::type.type"
    }
  }
}
